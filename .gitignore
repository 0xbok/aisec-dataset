# and development files from Hardhat, Foundry, Truffle, and other tools

# ===========================
# Node.js / NPM / Yarn
# ===========================
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.npm
.yarn-integrity
.pnp
.pnp.js

# ===========================
# Hardhat
# ===========================
cache/
artifacts/
deployments/
.hardhat/

# Hardhat ignores
coverage/
coverage.json
typechain/
typechain-types/

# ===========================
# Foundry
# ===========================
out/
cache_forge/
broadcast/
lib/
.forge-cache/
forge-cache/

# Foundry artifacts
*/out/
*/cache_forge/
*/broadcast/
*/lib/

